{
  "name": "asany-admin",
  "version": "0.2.38",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "main": "main.js",
  "homepage": ".",
  "scripts": {
    "estart": "electron .",
    "ebuild": "electron-builder",
    "postinstall": "husky install && umi g tmp && run-script-os",
    "postinstall:win32": "cd scripts && run.cmd",
    "postinstall:darwin:linux": "sh ./scripts/run.sh",
    "postinstall:default": "sh ./scripts/run.sh",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gql-gen": "graphql-codegen",
    "lint": "umi g tmp && npm run lint:js && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx,.gql",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx,.gql --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx,.gql --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*.{ts,tsx,graphql,gql}\" --end-of-line auto",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*.{ts,tsx,graphql,gql}\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "serve": "umi-serve",
    "tsc": "tsc --noEmit",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "build": {
    "productName": "FlowStream",
    "appId": "cn.asany.apps.FlowStream",
    "createDesktopShortcut": true,
    "directories": {
      "output": "builder-dist"
    },
    "npmRebuild": false,
    "files": [
      "main.js",
      "preload.js",
      "dist/**/*",
      "!node_modules/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "shortcutName": "电子屏幕"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
