{
  "name": "asany-admin",
  "version": "0.2.4",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "postinstall": "umi g tmp && run-script-os",
    "postinstall:win32": "cd scripts && run.cmd",
    "postinstall:darwin:linux": "sh ./scripts/run.sh",
    "postinstall:default": "sh ./scripts/run.sh",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gql-gen": "graphql-codegen",
    "lint": "umi g tmp && npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*.{ts,tsx,graphql}\" --end-of-line auto",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*.{ts,tsx,graphql}\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "serve": "umi-serve",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@asany/editor": "^0.2.17",
    "@asany/icons": "^0.2.3",
    "@asany/sortable": "^0.1.30",
    "@asany/tree": "^0.1.14",
    "@ckeditor/ckeditor5-build-balloon-block": "^31.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.3",
    "@popperjs/core": "^2.11.0",
    "@umijs/route-utils": "^2.0.4",
    "antd": "^4.16.13",
    "autosize": "^5.0.1",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "daterangepicker": "^3.1.0",
    "dropzone": "^5.9.3",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-react": "^0.2.3",
    "rc-field-form": "1.22.0",
    "react": "^17.0.2",
    "react-bootstrap": "2.0.3",
    "react-countup": "^6.1.0",
    "react-dev-inspector": "^1.7.1",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-helmet-async": "^1.2.0",
    "react-router-navigation-prompt": "^1.9.1",
    "react-use": "^17.3.1",
    "select2": "^4.1.0-rc.0",
    "sunmao": "^0.2.29",
    "sunmao-editor": "^0.1.15",
    "sweetalert2": "^11.2.1",
    "sweetalert2-react-content": "^4.2.0",
    "umi": "^3.5.20",
    "umi-serve": "^1.9.10"
  },
  "devDependencies": {
    "@asany/umi-plugin-apollo": "^0.1.6",
    "@asany/umi-plugin-app": "^0.1.28",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@types/autosize": "^4.0.0",
    "@types/daterangepicker": "^3.1.4",
    "@types/dropzone": "^5.7.4",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.3",
    "@types/jquery": "^3.5.9",
    "@types/lodash": "^4.14.177",
    "@types/md5": "^2.3.1",
    "@types/moment": "^2.13.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/select2": "^4.0.54",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-react": "^1.8.30",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.0",
    "puppeteer-core": "^12.0.1",
    "run-script-os": "^1.1.6",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "stylelint": "^14.1.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
